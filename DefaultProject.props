<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <References>$(SolutionDir)References\</References>
  </PropertyGroup>
  <PropertyGroup>
    <OutDir>$(ProjectDir)Output\Binaries\$(Configuration)\</OutDir>
    <IncludePath>$(SolutionDir)References\include;$(ProjectDir)Include;$(IncludePath)</IncludePath>
    <LibraryPath>$(IncludePath);$(SolutionDir)References\lib\x86;$(ProjectDir)lib;$(LibraryPath)</LibraryPath>
    <IntDir>$(ProjectDir)Output\Immediate\$(Configuration)\</IntDir>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <Link>
      <SubSystem>Windows</SubSystem>
    </Link>
    <ClCompile>
      <PreprocessorDefinitions>_WIN32;WIN32;%(PreprocessorDefinitions)</PreprocessorDefinitions>
    </ClCompile>
    <PostBuildEvent />
  </ItemDefinitionGroup>
  <ItemGroup>
    <BuildMacro Include="References">
      <Value>$(References)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>